  "license": "MIT",
  "description": "Due date calculator written in PHP",
  "keywords": [
    "php",
    "composer",
    "package",
    "duedate",
    "due",
    "calculator"
  ],
  "homepage": "https://github.com/shakahl/due-php",
  "authors": [
    {
      "name": "Soma Szélpál",
      "email": "szelpalsoma@gmail.com",
      "homepage": "https://github.com/shakahl",
      "role": "developer"
    }
  ],
  "bin": ["bin/due"],
  "require": {
    "php": ">=7.0",
    "symfony/console": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Shakahl\\Due\\TestCases\\": "tests/",
      "Shakahl\\Due\\": "src/Due/"
